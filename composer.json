{
    "name": "rtheunissen/guzzle-cache-handler",
    "description": "Guzzle 6/7 handler used to cache responses",
    "keywords": ["guzzle", "http", "handler", "cache", "psr7"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rudi Theunissen",
            "email": "rudolf.theunissen@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "rtheunissen/cache": "~0.1",
        "guzzlehttp/guzzle": "~6.0|~7.0",
        "psr/log": "~1.0|~2.0|~3.0"
    },
    "require-dev": {
        "doctrine/cache": "~1.4",
        "phpunit/phpunit": "^9.3",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Concat\\Http\\Handler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Concat\\Http\\Handler\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit "
    }
}
